<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ResourceConfiguration>
    <Policies>
        <Entry>
            <PolicyScope>PROXY_REQUEST</PolicyScope>
            <PolicyID>Callout</PolicyID>
        </Entry>
        <Entry>
            <PolicyScope>PROXY_RESPONSE</PolicyScope>
            <PolicyID>BuildResponse</PolicyID>
        </Entry>
        <Entry>
            <PolicyScope>TARGET_REQUEST</PolicyScope>
            <PolicyID>ParseServiceResponse</PolicyID>
        </Entry>
        <Entry>
            <PolicyScope>TARGET_REQUEST</PolicyScope>
            <PolicyID>BuiildRequest</PolicyID>
        </Entry>
        <Entry>
            <PolicyScope>TARGET_RESPONSE</PolicyScope>
            <PolicyID>ParserTargetResponse</PolicyID>
        </Entry>
    </Policies>
    <allowChildPaths>false</allowChildPaths>
    <basePath>{branchId}/location</basePath>
    <dummyPolicyConfigurationsForJson>
        <entry>
            <key>PROXY_REQUEST</key>
            <value/>
        </entry>
        <entry>
            <key>PROXY_RESPONSE</key>
            <value/>
        </entry>
        <entry>
            <key>TARGET_REQUEST</key>
            <value/>
        </entry>
        <entry>
            <key>TARGET_RESPONSE</key>
            <value/>
        </entry>
    </dummyPolicyConfigurationsForJson>
    <endpoints>
        <entry>
            <key>DefaultTarget</key>
            <value>1</value>
        </entry>
    </endpoints>
    <forwardBasePath>false</forwardBasePath>
    <loadBalancingStrategy>ROUND_ROBIN</loadBalancingStrategy>
    <resourceID>branches</resourceID>
    <streamingScope>NONE</streamingScope>
    <targetRules>
        <Entry>
            <Key>NoRule</Key>
            <Value>DefaultTarget</Value>
        </Entry>
    </targetRules>
    <wafEnabled>false</wafEnabled>
    <wafFilterClassName>org.owasp.esapi.waf.ESAPIWebApplicationFirewallFilter</wafFilterClassName>
    <wafFilterConfig/>
</ResourceConfiguration>
